	name = Ethan Sargent
[commit]
	gpgsign = false
[core]
	excludesfile = "$XDG_CONFIG_HOME/git/.gitignore_global"
	ignorecase = false
[alias]
	cm = commit -m
	co = checkout
	cam = commit -am
	st = status -uno
	pr = "!f() { exec az repos pr \"$@\"; }; f"
	repo = "!f() { exec az repos \"$@\"; }; f"
[merge]
	tool = nvimdiff2
